{
  "name": "cliberry-schematics",
  "version": "0.0.2",
  "description": "Collection for schematics template used in cliberry/schematics/angular cli that creates empty template for cliberry/schematics/angular.",
  "main": "index",
  "schematics": "./src/collection.json",
  "scripts": {
    "schematics": "schematics",
    "cliberry": "cliberry",
    "build": "yarn --dev && tsc -p tsconfig.json",
    "test": "yarn build && jasmine src/**/*.spec.js"
  },
  "engines": {
    "node": ">=8.9"
  },
  "homepage": "https://github.com/tgorka/cliberry-schematics",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgorka/cliberry-schematics.git"
  },
  "private": false,
  "author": {
    "name": "Tomasz Gorka",
    "email": "tomasz@gorka.org.pl"
  },
  "keywords": [
    "schematics",
    "cliberry",
    "template"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "^0.8.1",
    "@angular-devkit/schematics": "^0.8.1",
    "@schematics/angular": "^0.8.1"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.8.1",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.9.4",
    "cliberry": "^0.1.4",
    "jasmine": "^3.2.0",
    "typescript": "^3.0.3"
  }
}
